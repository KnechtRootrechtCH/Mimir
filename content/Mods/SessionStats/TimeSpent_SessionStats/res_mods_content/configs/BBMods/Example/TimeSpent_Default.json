{
	${"Default/Colors.json"},
	${"Default/Translation.json"},

	"SkinDescription": {
		// Автор конфигурации
		"Author": "ShuraBB",
		// Название конфигурации
		"Name": "TimeSpent Default",
		// Версия конфигурации
		"Version": "2.0",
		// Описание конфигурации
		"Description": "2 секции статистики, все типы боев, результаты последнего боя, общая статистика по картам и танкам, альтернативные текстовые кнопки."
	},
	// Переключатель загрузки XVM таблицы границ диапазонов. true - загружать, false - использовать свою таблицу.
	"LoadXVMColors": true,

	// Настройка скорости пролистывания сообщений в системном канале. (Стандартное значение: 10) 
	"ScrollStep": 50,

	// Время отображения PopUp сообщения c результатами боя в ангаре. Стандартное значение 6000.0
	"PopUpResultLife": 12000.0,

	// Время "растворения" PopUp сообщения c результатами боя в ангаре.  Стандартное значение 2000.0
	"PopUpResultHiding": 1000.0,

	// Переключатель вывода результатов боя в ангаре. false - результаты мода, true - оригинальные результаты.
	"OriginalResults": false,

	// Переключатель вывода результатов предыдущего незаконченного боя в боевой чат. true - выводить, false - не выводить.
	"InBattleResult": true,

	// Настройка сессий результатов статистики.
	// Префикс макросов статистики (см. MacrosList.txt) формируется относительно названий, указанных в этой секции.
	// Например: {{Current_WN8}} выводит расчёт рейтинга по типам боёв, указанных в секции "Current".
	// Количество и название секций могут быть произвольными.
	"StatisticSessions": {
		// Основные настройки результатов сессии, выводимых макросами с префиксом {{Current_*}}
		"Current": {
			// Настройка автоматического сброса статистики.
			// "ResetAfter" - Автоматически обнулять статистику по истечении указанного кол-ва дней. Если текущая дата превышает SessionStart + ResetAfter, то вне зависимости от ResetTime, статистика будет сброшена. При ResetAfter = 0 статистика не будет сбрасываться автоматически.
			// "ResetTime" - Час после которого обнуляется статистика. Статистика обнулится если текущий час >= ResetTime
			// "LocalTime" - true/false - Рассчитывать ResetTime по местному времени или по UTC. 0 часов по UTC соответствует времени сброса статистики у ВГ
			"Reset": {"ResetAfter": 0, "ResetTime": 0, "LocalTime": false},

			// Настройка типов боев учитываемых данной сессией. Список в конце файла.
			"BattleTypes": [0,1,3,4,5,7,12,13,14,15,16,20,21,22,24],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
		},
		"Overall": {
			"Reset": {"ResetAfter": 0, "ResetTime": 0, "LocalTime": false},
			"BattleTypes": [0,1,3,4,5,7,12,13,14,15,16,20,21,22],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
		}
	},

	// Строка выводимая макросом {{BattleResultName}} в зависимости от результата боя.
	"ResultName": {"Win": "Победа", "Defeat": "Поражение", "Draw": "Ничья"},

	// Шаблон сообщений результатов предыдущего незаконченного боя в боевой чат.
	"InBattleResultPattern": [
		"<p align='center'><font color='{{c:BattleResult}}' size='25'>{{BattleResultName}}</font><br><font color='#E0E0E0' size='18'>{{BattleMapName}} - {{BattleTankName}}</font></p>"
	],

	// Шаблоны для вывода сообщений результатов боя.
	"ResultPattern": {
		// "icon": "",
		// "bgIcon": "",
		"template": ["{{um:Results}}"]
	},

	// Форматирование списка достижений
	"Achievements": {
		// Форматирование списка достижений в бою
		"Battle": {
			// Заголовок списка достижений
			"Header": "\n<p align='center'><font color='#D3D3D3'><b>Достижения:</b></font></p><textformat leading='-3' tabstops='1,20'>",
			// Шаблон одной строки списка достижений.
			"Body": "\t<img src='{{AchIcon}}' width='26' height='26' vspace='-16'>\t{{AchName}} ({{AchValue}})",
			// Разделитель списка достижений
			"Separator": "\n",
			// Конец списка достижений
			"Footer": "</textformat>\n"
		},
		// Форматирование списка достижений за сессию статистики "Current"
		"Current": {
			"Header": "\n<p align='center'><font color='#D3D3D3'><b>Достижения:</b></font></p><textformat leading='-3' tabstops='1,20'>",
			"Body": "\t<img src='{{AchIcon}}' width='38' height='40' vspace='-16'>\t{{AchName}} - {{AchValue}}",
			"Separator": "\n",
			"Footer": "</textformat>\n"
		}
	},

	// Статистика танка в панели ТТХ и в карусели танков
	"StatisticbyTank": {
		// Состояние по умолчанию раздела статистики в панели ТТХ. true - раскрыт, false - свернут
		"ShowDefault": false,
		// Из какой сессии брать статистику для отображения
		"Sessions": "Current",
		// Значение отображаемое в индикаторной полосе. Возможные значения: 'DRating', 'Damage', 'Xp', 'WN8', 'EFF'
		"Indicator": "DRating",
		// Текстовый заголовок. Поддерживаются txt: макросы
		"TitleText": "<font face='$TitleFont' size='15' color='#E9E2BF'>txt:ByTankTitle</font>",
		// Данные в строке заголовка. Поддерживаются всем макросы. Если параметр не задан, выводятся данные в зависимости от параметра "Indicator"
		"TitleData": "",
		// Шаблон для отображения данных в панели ТТХ
		"template": [
			// Одна строка данных. Формат [D1, D2, D3]
			// D1 - раздел данных. Поддерживаются всем макросы
			// D2 - раздел иконки. Макросы не поддерживаются
			// D3 - раздел описания. Поддерживаются txt: макросы
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{DamageRating:2f}}%</font>", "../maps/icons/statistic/rating24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankDr</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{WinCounter:r}}% / {{WinsEfficiency}}%</font>", "../maps/icons/statistic/wins24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankWinr</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{BattleCounter}} / {{WinCounter}}</font>", "../maps/icons/statistic/battles24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankBattle</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{BattleLevelAvg:1f}}</font>", "../maps/icons/statistic/battles24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankBL</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{MaxFrags}} / {{KillsAvg:1f}}</font>", "../maps/icons/statistic/battles24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankMaxKill</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{DamageDealtAvg}} / {{AvgDamage}}</font>", "../maps/icons/statistic/avgDamage24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankDmg</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{XPAvg}} / {{AvgXP}}</font>", "../maps/icons/statistic/avgExp24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankXP</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{MaxDamage}} / {{MaxXp}}</font>", "../maps/icons/statistic/armorUsing24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankMax</font>"],
			["<font face='$FieldFont' size='14' color='#E9E2BF'>{{BattleTimer:t}}</font>", "../maps/icons/statistic/daysInClan24.png", "<font face='$FieldFont' size='14' color='#8C8C7E'>txt:ByTankTime</font>"]
		],
		// Информационная строка в танковой карусели. Поддерживаются всем макросы
		"Carousel": [
			"<font face='$FieldFont' size='14' color='#E9E2BF'>",
			"<img src='img://gui/maps/icons/library/proficiency/class_icons_{{MarkOfMastery}}_small.png' width='24' height='24' vspace='-7'>\t",
			"<img src='img://gui/maps/icons/statistic/wins24_gray.png' width='24' height='24' vspace='-7'><font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>\t",
			"{{up:MarksOnGunIcon}}",
			"</font>"
		]
	},

	// Страницы с выводимой информацией. Имена страниц "NoBattle" и "LastBattle" используются для целей мода.
	"Pages": {
		// Имя страницы которая будет отображаться первой
		"First": "LastBattle",

		// Шаблон сообщения при отсутствии боев.
		"NoBattle": {
			// шаблон выводимой информации
			"template": [
				"<p align='center'><font color='#E0E0E0' size='16'>{{UserName}}\nРезультаты боя пока не доступны.</font></p>",
				"\n\n<font size='12' color='#545454'>",
				"Мод: <a href='event:{{ModURL}}'><font color='#545454'>{{ModVersion}}</font></a>\n",
				"Скин: '{{SkinName}}' ver:{{SkinVersion}}\n",
				"Автор скина: {{SkinAuthor}}\n\n",
				"<p align='justify'>{{SkinDescription}}</p></font>",
				"\n<p align='center'>{{um:ButtonLastBattle}}  {{um:ButtonCurrent}}  {{um:ButtonOverall}}  {{um:ButtonMaps}}  {{um:ButtonTanks}}</p>"
			],
			// Для использования стандартной иконки - за комментируйте параметр, для отключения иконки - задайте значение ""
			// Путь к боковой иконке.
			"icon": "",
			// Путь к фоновой картике
			"bgIcon": ""
		},
		// Шаблон окна для результатов последнего боя.
		"LastBattle": {
			"template": [
				"<p align='center'><font size='16' color='#33cc00'><b>Результаты последнего боя:</b></font></p><textformat tabstops='90'>",
				"Карта:\t<font color='#D3D3D3'>{{BattleMapName}}</font>\n",
				"Танк:\t<font color='#D3D3D3'>{{BattleTankName}}</font>{{up:BattleMastery}}\n",
				"Результат:\t<font color='#D3D3D3'>{{BattleResultName}}</font>\n<textformat tabstops='150'>",
				"Начислено за бой:\t<font color='#D3D3D3'>{{BattleCredits}}</font>\n",
				"Ремонт техники:\t<font color='#D3D3D3'>{{BattleRepairCost}}</font>\n",
				"Пополнение БК:\t<font color='#D3D3D3'>{{BattleLoadCost}} / {{BattleLoadCostG}}</font>\n",
				"Пополнение СНР:\t<font color='#D3D3D3'>{{BattleEquipCost}} / {{BattleEquipCostG}}</font>\n",
				"Чистый доход:\t<font color='#D3D3D3'>{{BattleCreditDebit}}</font>\n",
				"Полученный опыт:\t<font color='#D3D3D3'>{{BattleXP}} {{BattleDailyXPFactor}}</font>\n",
				"Опыт экипажу:\t<font color='#D3D3D3'>{{BattleTmenXP}}</font>\n",
				"Свободный опыт:\t<font color='#D3D3D3'>{{BattleFreeXP}}</font>\n",
				"Нанесенный урон:\t<font color='#D3D3D3'>{{BattleDamageDealt}}</font>\n",
				"Урон по засвету:\t<font color='#D3D3D3'>{{BattleAssistedRadio}}</font>\n",
				"Урон по гусле:\t<font color='#D3D3D3'>{{BattleAssistedTrack}}</font>\n",
				"Урон >300м:\t<font color='#D3D3D3'>{{BattleSniperDamage}}</font>\n",
				"Полученный урон:\t<font color='#D3D3D3'>{{BattleDamageReceived}}</font>\n",
				"Натанкованно:\t<font color='#D3D3D3'>{{BattleBlockedByArmor}}</font>\n",
				"Обнаружено:\t<font color='#D3D3D3'>{{BattleSpotted}}</font>\n",
				"Уничтожено:\t<font color='#D3D3D3'>{{BattleKills}}</font>\n",
				"Повреждено:\t<font color='#D3D3D3'>{{BattleDamaged}}</font>\n",
				"Ко.урона:\t<font color='#D3D3D3'>{{BattleDamageRatio}}</font>\n",
				"Ко.урона по WN8:\t<font color='#D3D3D3'>{{BattleRatioDamageWN8:2f}}</font>\n",
				"Ко.урона по WN8K:\t<font color='#D3D3D3'>{{BattleRatioDamageWN8K:2f}}</font>\n",
				"Ко.уничтожения:\t<font color='#D3D3D3'>{{BattleFragsRatio}}</font>\n",
				"Акк. Ранг/Шевроны:\t<font color='#D3D3D3'>{{BattleAccRank}} / {{BattleChevron}}</font>\n",
				"Танк Ранг/Шевроны:\t<font color='#D3D3D3'>{{BattleVehRank}} / {{BattleChevron}}</font>\n",
				"Шеврон получен:\t<font color='#D3D3D3'>{{BattleRankChange}}</font>\n",
				"{{up:BattleDamageRating}}",
				"% попаданий:\t<font color='#D3D3D3'>{{BattleHitsRatio}}%</font>\n",
				"% попаданий с уроном:\t<font color='#D3D3D3'>{{BattleDamageHitsRatio}}%</font>\n",
				"% выстрелов с уроном:\t<font color='#D3D3D3'>{{BattleShotsRatio}}%</font>\n",
				"Уровень боя:\t<font color='#D3D3D3'>{{BattleLevel}}</font>\n",
				"Был в бою:\t<font color='#D3D3D3'>{{BattleDuration:t}}</font>\n",
				"Время жизни:\t<font color='#D3D3D3'>{{BattleLifeTime:t}}</font>\n",
				"Длительность боя:\t<font color='#D3D3D3'>{{BattleDurationReal:t}}</font>\n",
				"Рейтинг EFF:\t<font color='#D3D3D3'>{{BattleEFF}} ({{BattleXEFF}})</font>\n",
				"Рейтинг WN6:\t<font color='#D3D3D3'>{{BattleWN6}} ({{BattleXWN6}})</font>\n",
				"Рейтинг WN7:\t<font color='#D3D3D3'>{{BattleWN7}} ({{BattleXWN7}})</font>\n",
				"Рейтинг WN8:\t<font color='#D3D3D3'>{{BattleWN8}} ({{BattleXWN8}})</font>\n",
				"Рейтинг WN8К:\t<font color='#D3D3D3'>{{BattleWN8K}} ({{BattleXWN8K}})</font>\n",
				"Рейтинг BR:\t<font color='#D3D3D3'>{{BattleBR}}</font>\n",
				"{{BattleAchievements}}",
				"\n\n<p align='center'>{{um:ButtonCurrent}}  {{um:ButtonOverall}}  {{um:ButtonMaps}}  {{um:ButtonTanks}}</p>"
			],
			"icon": "img://gui/maps/icons/library/ClanBattleResultIcon-1.png",
			"bgIcon": "img://../res_mods/configs/BBMods/Default/bg.png"
		},
		"Current": {
			"template": [
				"<p align='center'><font size='16' color='#33cc00'><b>Cтатистика текущей сессии:</b></font></p>",

				"\n<p align='center'><font color='#ffffff'><b>По картам:</b></font></p><textformat tabstops='120,150,190'>",
				"{{Current_MapsStat}}",

				"\n<p align='center'><font color='#ffffff'><b>По танкам:</b></font></p><textformat tabstops='125,155,190'>",
				"{{Current_TanksStat}}",

				"\n<p align='center'><font color='#ffffff'><b>Статистика:</b></font></p><textformat tabstops='110,162'>",
				"Сессия начата:\t<b>{{Current_SessionStart}}</b>\n",
				"Боев / Побед:\t<b>{{Current_BattleCounter}} /{{Current_WinCounter}} <font color='{{c:Current_NumWinRate}}'>{{Current_NumWinRate:0f}}</font>\t( <font color='{{c:Current_WinCounter:r}}'>{{Current_WinCounter:r}}%</font> )</b>\n",
				"Ср.уровень боев:\t<b>\t( <font color='{{c:Current_BattleLevelAvg}}'>{{Current_BattleLevelAvg:1f}}</font> )</b>\n",
				"Проведено в боях:\t<b>{{Current_BattleTimer:t}}\t( <font color='{{c:Current_BattleTimerAvg}}'>{{Current_BattleTimerAvg:t}}</font> )</b>\n",
				"Урон:\t<b>{{Current_DamageDealt}}\t( <font color='{{c:Current_DamageDealtAvg}}'>{{Current_DamageDealtAvg}}</font> )</b>\n",
				"Опыт:\t<b>{{Current_OriginalXP}}\t( <font color='{{c:Current_OriginalXPAvg}}'>{{Current_OriginalXPAvg}}</font> )</b>\n",
				"Уничтожено:\t<b>{{Current_Kills}}\t( <font color='{{c:Current_KillsAvg}}'>{{Current_KillsAvg:2f}}</font> )</b>\n",
				"Обнаружено:\t<b>{{Current_Spotted}}\t( <font color='{{c:Current_SpottedAvg}}'>{{Current_SpottedAvg:2f}}</font> )</b>\n",
				"Заработано:\t<b>{{Current_Credits}}\t( <font color='{{c:Current_CreditsAvg}}'>{{Current_CreditsAvg}}</font> )</b>\n",
				"Пополнение БК:\t<b>{{Current_LoadCost}}\t( <font color='{{c:Current_LoadCostAvg}}'>{{Current_LoadCostAvg}}</font> )</b>\n",
				"Пополнение СНР:\t<b>{{Current_EquipCost}}\t( <font color='{{c:Current_EquipCostAvg}}'>{{Current_EquipCostAvg}}</font> )</b>\n",
				"Доход:\t<b>{{Current_CreditDebit}}\t( <font color='{{c:Current_CreditDebitAvg}}'>{{Current_CreditDebitAvg}}</font> )</b>\n",
				"Урон >300м:\t<b>{{Current_SniperDamage}}\t( <font color='{{c:Current_SniperDamageAvg}}'>{{Current_SniperDamageAvg}}</font> )</b>\n",
				"Помощь в уроне:\t<b>{{Current_Assisted}}\t( <font color='{{c:Current_AssistedAvg}}'>{{Current_AssistedAvg}}</font> )</b>\n",
				"Урон по засвету:\t<b>{{Current_AssistedRadio}}\t( <font color='{{c:Current_AssistedRadioAvg}}'>{{Current_AssistedRadioAvg}}</font> )</b>\n",
				"Урон по гусле:\t<b>{{Current_AssistedTrack}}\t( <font color='{{c:Current_AssistedTrackAvg}}'>{{Current_AssistedTrackAvg}}</font> )</b>\n",
				"Заблокированно:\t<b>{{Current_BlockedByArmor}}\t( <font color='{{c:Current_BlockedByArmorAvg}}'>{{Current_BlockedByArmorAvg}}</font> )</b>\n",

				"Шевронов:\t<b>{{Current_Chevron}}\t( <font color='{{c:Current_ChevronAvg}}'>{{Current_ChevronAvg}}</font> )</b>\n",
				"Шевр. получено:\t<b>{{Current_ChevronIn}}\t( <font color='{{c:Current_ChevronInAvg}}'>{{Current_ChevronInAvg}}</font> )</b>\n",
				"Шевр. потеряно:\t<b>{{Current_ChevronOut}}\t( <font color='{{c:Current_ChevronOutAvg}}'>{{Current_ChevronOutAvg}}</font> )</b>\n",

				"Рейтинг WGR:\t<b>{{WGR}}\t( <font color='{{c:XWGR}}'>{{XWGR}}</font> )</b>\n",
				"Рейтинг EFF:\t<b>{{Current_EFF}}\t( <font color='{{c:Current_XEFF}}'>{{Current_XEFF}}</font> )</b>\n",
				"Рейтинг WN6:\t<b>{{Current_WN6}}\t( <font color='{{c:Current_XWN6}}'>{{Current_XWN6}}</font> )</b>\n",
				"Рейтинг WN7:\t<b>{{Current_WN7}}\t( <font color='{{c:Current_XWN7}}'>{{Current_XWN7}}</font> )</b>\n",
				"Рейтинг WN8:\t<b>{{Current_WN8}}\t( <font color='{{c:Current_XWN8}}'>{{Current_XWN8}}</font> )</b>\n",
				"Рейтинг WN8K:\t<b>{{Current_WN8K}}\t( <font color='{{c:Current_XWN8K}}'>{{Current_XWN8K}}</font> )</b>\n",
				"Рейтинг БС:\t<b>{{Current_BS}}\t( <font color='{{c:Current_XBS}}'>{{Current_XBS}}</font> )</b>\n",
				"Рейтинг BR:\t<b>{{Current_BR}}</b>\n",
				"Последний бой:\t<b>{{LastBattleTime}}</b>\n",
				"Общий онлайн:\t<b>{{Current_SessionOnline}}</b>\n",
				"С начала сессии:\t<b>{{Current_SessionTime}}</b>\n",

				"\n<p align='center'><font color='#ffffff'><b>Влияние сессии на аккаунт:</b></font></p><textformat tabstops='100,170'>",
				"Серебро:\t<b>{{Current_DossCredits}}\t<font color='{{c:Current_DiffCredits}}'>{{Current_DiffCredits:0f}}</font></b><textformat tabstops='130,170'>\n",
				"Ранг аккаунта:\t<b>{{Current_DossAccRank}}\t<font color='{{c:Current_DiffAccRank}}'>{{Current_DiffAccRank:0f}}</font></b>\n",
				"Очки ранга:\t<b>{{Current_DossRankPoints}}\t<font color='{{c:Current_DiffRankPoints}}'>{{Current_DiffRankPoints:0f}}</font></b>\n",
				"Золото:\t<b>{{Current_DossGold}}\t<font color='{{c:Current_DiffGold}}'>{{Current_DiffGold:0f}}</font></b>\n",
