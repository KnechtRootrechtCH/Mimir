		"template": ["{{Name}}:\t{{BattleCounter}}/{{WinCounter}}\t<font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>\t<font color='{{c:BattleTimerAvg}}'>{{BattleTimerAvg:t}}</font>\n"]
