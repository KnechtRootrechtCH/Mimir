{
	${"YasenKrasen/Translation.json"},
	${"YasenKrasen/StatisticbyTank.json"},

	"ScrollStep": 50,
	"PopUpResultLife": 12000.0,
	"PopUpResultHiding": 1000.0,
	"OriginalResults": false,
	"InBattleResult": true,

	"StatisticSessions": {
		"Current": {
			"Reset": {"ResetAfter": 7, "ResetTime": 3},
			"BattleTypes": [1,3,7,13,20,21,22,24],
			"SectionsList": ["Statistic", "Tanks", "Maps"]
		}
	},

	"ResultName": {"Win": "txt:Win", "Defeat": "txt:Defeat", "Draw": "txt:Draw"},

	"InBattleResultPattern": ["{{um:imgBattles}}<font size='13'><font color='{{c:BattleResult}}'>{{BattleResultName}}</font>   {{um:imgBattles}}<font color='#FFE6B3'>{{BattleMapName}}</font><br>{{um:imgTank2}}<font color='#CDC673'>{{BattleTankName}}</font>   {{um:imgXpIcon}}<font color='#A0CFFF'>{{BattleXP}}</font>   {{um:imgCredits}}<font color='{{c:BattleCredits}}'>{{BattleCredits}}</font></font>"],

	"Pages": {
		"First": "Current",
		"NoBattle": {
			"template": [
				"<p align='center'><font color='#E0E0E0' size='16'>{{UserName}}\ntxt:NoBattle</font></p>",
				"\n\n<font size='12' color='#CDC673'>",
				"txt:Mod: <a href='event:{{ModURL}}'><font color='#CDC673'>{{ModVersion}}</font></a>\n",
				"txt:Skin: '{{SkinName}}' ver:{{SkinVersion}}\n",
				"txt:Autor: {{SkinAuthor}}\n\n",
				"<p align='justify'>{{SkinDescription}}</p></font>"
			]
		},

		"Current": {
			"template": [
				"<font size='13' color='#AE6E2D'>txt:Head1</font>\n{{um:imgLine}}\n",
				"<textformat tabstops='[90,110,165]'><font size='12' color='#CDC673'>",
				"txt:Credits\t{{um:imgCredits}}\t<font color='#FFFFFF'>{{Current_Credits}}</font>\n",
				"txt:CreditExpenses\t{{um:imgCredits}}\t<font color='#FFFFFF'>{{Current_Expenses}}</font>\n",
				"txt:CreditDebit\t{{um:imgCredits}}\t<font color='{{c:Current_CreditDebit}}'>{{Current_CreditDebit}}</font></font></textformat>\n<font size='8'>\n</font>",

				"<font size='13' color='#AE6E2D'>txt:Head2</font>\n{{um:imgLine}}</font>\n",
				"<textformat tabstops='[90,110,160]' leading='0'><font size='12'>txt:WinsBattles\t{{um:imgBattles}}\t<b><font color='{{c:Current_NumWinRate}}'>{{Current_WinCounter}}</font><font color='#FFFFFF'> / {{Current_BattleCounter}}</font>\t(<font color='{{c:Current_WinCounter:r}}'>{{Current_WinCounter:r}}%</font>)</b>\n",
				"txt:OriginalXP\t{{um:imgXpIcon}}\t<b><font color='#FFFFFF'>{{Current_OriginalXP}}</font>\t(<font color='{{c:Current_OriginalXPAvg}}'>{{Current_OriginalXPAvg}}</font>)</b>\n",
				"txt:XP\t{{um:imgXp}}\t<b><font color='#FFFFFF'>{{Current_XP}}</font>\t(<font color='{{c:Current_XPAvg}}'>{{Current_XPAvg}}</font>)</b>\n",
				"txt:FreeXP\t{{um:imgFreeXp}}\t<b><font color='#FFFFFF'>{{Current_FreeXP}}</font>\t(<font color='{{c:Current_FreeXPAvg}}'>{{Current_FreeXPAvg}}</font>)</b>\n<font size='8'>\n</font>",
				
				"txt:Kills\t{{um:imgKills}}\t<b><font color='#FFFFFF'>{{Current_Kills}}</font>\t(<font color='{{c:Current_KillsAvg}}'>{{Current_KillsAvg:1f}}</font>)</b>\n",
				"txt:Death\t{{um:imgDeath}}\t<b><font color='#FFFFFF'>{{Current_DeathCount}}</font>\t(<font color='{{c:Current_DeathCountAvg}}'>{{Current_DeathCountAvg:1f}}</font>)</b>\n",
				"txt:ShotsHits\t{{um:imgShots}}\t<b><font color='#FFFFFF'>{{Current_Shots}} / {{Current_Hits}}</font>\t(<font color='{{c:Current_HitsRatio}}'>{{Current_HitsRatio:1f}}%</font>)</b>\n",
				"txt:Piercings\t{{um:imgPiercings}}\t<b><font color='#FFFFFF'>{{Current_Piercings}}</font>\t(<font color='{{c:Current_DamageHitsRatio}}'>{{Current_DamageHitsRatio:1f}}%</font>)</b>\n",
				"txt:DamageDealt\t{{um:imgDamage}}\t<b><font color='#FFFFFF'>{{Current_DamageDealt}}</font>\t(<font color='{{c:Current_DamageDealtAvg}}'>{{Current_DamageDealtAvg}}</font>)</b>\n",
				"txt:DamageReceived\t{{um:imgDamage}}\t<b><font color='#FFFFFF'>{{Current_DamageReceived}}</font>\t(<font color='#FFFFFF'>{{Current_DamageReceivedAvg}}</font>)</b>\n",
				"txt:Spotted\t{{um:imgSpotted}}\t<b><font color='#FFFFFF'>{{Current_Spotted}}</font>\t(<font color='{{c:Current_SpottedAvg}}'>{{Current_SpottedAvg:1f}}</font>)</b>\n",
				"txt:Capture\t{{um:imgCapture}}\t<b><font color='#FFFFFF'>{{Current_CapturePoints}}</font>\t(<font color='#FFFFFF'>{{Current_CapturePointsAvg:1f}}</font>)</b>\n",
				"txt:Defense\t{{um:imgDefense}}\t<b><font color='#FFFFFF'>{{Current_DroppedCapturePoints}}</font>\t(<font color='#FFFFFF'>{{Current_DroppedCapturePointsAvg:1f}}</font>)</b>\n",
				"txt:inBattles\t{{um:imgBattleTimer}}\t<b><font color='#FFFFFF'>{{Current_BattleTimer:t}}</font>\t(<font color='{{c:Current_BattleTimerAvg}}'>{{Current_BattleTimerAvg:t}}</font>)</b>\n",
				"txt:LevelAvg\t{{um:imgTankLevel}}\t\t<b>(<font color='{{c:Current_BattleLevelAvg}}'>{{Current_BattleLevelAvg:1f}}</font>)</b>\n<font size='8'>\n</font>",

				"WN8 / WN8K:\t<b><font color='{{c:Current_WN8}}'>{{Current_WN8}}</font> (<font color='{{c:Current_WN8}}'>{{Current_XWN8}}</font>)\t\t<font color='{{c:Current_WN8K}}'>{{Current_WN8K}}</font> (<font color='{{c:Current_WN8K}}'>{{Current_XWN8K}}</font>)</b>\n",
				"WGR / EFF:\t<b><font color='{{c:WGR}}'>{{WGR}}</font> (<font color='{{c:WGR}}'>{{XWGR}}</font>)\t\t<font color='{{c:Current_EFF}}'>{{Current_EFF}}</font> (<font color='{{c:Current_EFF}}'>{{Current_XEFF}}</font>)</b>\n",
				"</font></textformat>",

				"<p align='center'>\n{{um:ButtonMaps}}   {{um:ButtonTanks}}   {{um:ButtonReset}}</p>"
			]
		},

		"Maps": {
			"template": [
				"<font size='13'><textformat tabstops='30,137,160,200'>",
				"\t<a href='event:Sort:Name'><font color='#AE6E2D'>txt:Map</font></a>\t<a href='event:Sort:BattleCounter'><img src='img://gui/maps/icons/statistic/battles24.png' width='20' height='20' vspace='-9'></a>",
				"\t<a href='event:Sort:WinCounter:r'><img src='img://gui/maps/icons/statistic/wins24.png' width='22' height='22' vspace='-9'></a>\t<a href='event:Sort:BattleLevelAvg'><font color='#AE6E2D'>Ур</font></a></textformat></font>\n",
				"{{um:imgLine2}}\n",
				"<textformat tabstops='145,165,200' leading='0'><font size='12' color='#FFFFFF'>{{Current_MapsStat}}</font></textformat>",
				"<p align='center'>\n{{um:ButtonStat}}   {{um:ButtonTanks}}</p>"
			]
		},

		"Tanks": {
			"template": [
				"<font size='13'><textformat tabstops='25,88,113,153,192'>",
				"\t<a href='event:Sort:Name'><img src='img://gui/maps/icons/buttons/Tank-ico.png' width='26' height='11' vspace='-3'></a>",
				"\t<a href='event:Sort:BattleCounter'><img src='img://gui/maps/icons/statistic/battles24.png' width='22' height='22' vspace='-9'></a>",
				"\t<a href='event:Sort:WinCounter:r'><img src='img://gui/maps/icons/statistic/wins24.png' width='22' height='22' vspace='-9'></a>",
				"\t<a href='event:Sort:DamageDealtAvg'><img src='img://gui/maps/icons/statistic/avgDamage24.png' width='22' height='22' vspace='-9'></a>\t<a href='event:Sort:WN8'><font color='#AE6E2D'>WN8</font></a></textformat></font>\n",
				"{{um:imgLine2}}\n",
				"<textformat tabstops='21,97,115,152,190'><font size='12' color='#FFFFFF'>{{Current_TanksStat}}</font></textformat>",
				"<p align='center'>\n{{um:ButtonStat}}   {{um:ButtonMaps}}</p>"
			]
		}
	},

	"Current_MapsStat": {
		"SortKey": "BattleCounter#des",
		"template": ["<font color='#FFE6B3'>{{Name}}</font>\t{{BattleCounter}}\t<font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>\t<font color='{{c:BattleLevelAvg}}'>{{BattleLevelAvg:1f}}</font>\n"]
	},

	"Current_TanksStat": {
		"SortKey": "BattleCounter#des",
		"template": [
			"{{up:MarkOfMastery}}\t<font color='#FFE6B3'>{{TankNameShort}}</font>",
			"\t{{BattleCounter}}\t<font color='{{c:WinCounter:r}}'>{{WinCounter:r}}%</font>",
			"\t<font color='{{c:DamageDealtAvg}}'>{{DamageDealtAvg}}</font>\t<font color='{{c:XWN8}}'>{{WN8}}</font>\n"
		]
	},

	"Current_BattleTimer": "%H:%M:%S",
	"Current_SessionOnline": "%H txt:hour %M txt:min %S txt:sec",

	"UserMacros": {
		"um:ButtonStat": ["<a href='event:Page:Current'><b>txt:ButtonStat</b></a>"],
