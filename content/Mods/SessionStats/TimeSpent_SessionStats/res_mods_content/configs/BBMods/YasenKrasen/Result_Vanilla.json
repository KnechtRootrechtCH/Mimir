{
	"SkinDescription": {
		"Author": "_Jeso_ adapted by ShuraBB",
		"Name": "YasenKrasen Vanilla",
		"Version": "1.1",
		"Description": "txt:Description"
	},

	"ResultPattern": {
		// "icon": "",
		"bgIcon": "img://../res_mods/configs/BBMods/YasenKrasen/img/Bg.png",
		"template": [
			"<font color='{{c:BattleResult}}' size='18'><b>{{BattleResultName}}</b></font>\n\n",
			"<font size='12'>txt:Map2 <font color='#CDC673'>{{BattleMapName}}</font>\n",
			"txt:Vehicle <font color='#CDC673'>{{BattleTankName}}</font>\n",
			"txt:BattleXP <font color='#FFFFFF'>{{BattleXP}}</font>{{up:BattleDailyXPFactor}}\ttxt:BattleCredits <font color='#FFFFFF'>{{BattleCredits}}</font>\n",
			"{{BattleAchievements}}\n",
			"<textformat tabstops='[70,135]'><font color='#CDC673'>",
			"txt:Repair\t<font color='#FFFFFF'>-{{BattleRepairCost}} {{um:imgCredits}}</font>\n",
			"txt:Expenses\t<font color='#FFFFFF'>-{{BattleExpenses}} {{um:imgCredits}}</font>\n",
			"txt:Total\t<font color='{{c:BattleCreditDebit}}'>{{BattleCreditDebit}} {{um:imgCredits}}</font>\tWN8:</font> <font color='{{c:BattleWN8}}'>{{BattleWN8}}</font> (<font color='{{c:BattleWN8}}'>{{BattleXWN8}}</font>)\n",
			"</textformat></font>",
			"<p align='center'>\n{{um:ButtonDetailed}}</p>"
		]
	},

	"UserPattern": {
		"up:BattleDailyXPFactor": [" {{BattleDailyXPFactor}} ", ""],
		"up:MarkOfMastery": ["<img src='img://gui/maps/icons/library/proficiency/class_icons_{{MarkOfMastery}}.png' width='20' height='20' vspace='-6'>", ""]
	},

	"Achievements": {
		"Battle": {
			"Header": "txt:Achievements <font color='#FFFFFF'>",
			"Body": "{{AchName}}",
			"Separator": ", ",
			"Footer": "</font>\n"
		}
	}
}